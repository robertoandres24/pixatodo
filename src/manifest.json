{
  "name": "Pixatodo",
  "description": "Replace your New Tab page with a simple to-do list, plus beautiful wallpapers that you can change with a click",
  "version": 1.2,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "pixatodo",
    "default_popup": "popup/popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
